[begin] (fluxbox)
   [include] (/var/cache/xdg-menu/fluxbox/boxrc)
   #[include] (~/.fluxbox/xdg_menu)
   [submenu] (Autogenerated)
   [include] (~/.fluxbox/menu)
   [end]
   [submenu] (Fluxbox)
     [exec] (Run) {fbrun} 
     [exec] (Window name) {xprop WM_CLASS|cut -d \" -f 2|xmessage -file - -center} 
     [exec] (Screenshot - JPG) {import screenshot.jpg && display -resize 50% screenshot.jpg} </usr/share/icons/hicolor/64x64/apps/kontact-import-wizard.png>
     [exec] (Screenshot - PNG) {import screenshot.png && display -resize 50% screenshot.png} </usr/share/icons/hicolor/64x64/apps/kontact-import-wizard.png>
     [separator] 
     [exec] (Lock screen) {xscreensaver-command -lock} 
     [separator] 
     [reconfig] (Reload config) 
     [restart] (Restart) 
     [exec] (About) {(fluxbox -v; fluxbox -info | sed 1d) | xmessage -file - -center} 
     [exec] (Fluxbox panel) {fbpanel -p toolbar}
     [separator] 
     [submenu] (System Styles) {Choose a style...}
        [stylesdir] (/usr/share/fluxbox/styles)
     [end]
     [submenu] (User Styles) {Choose a style...}
        [stylesdir] (~/.fluxbox/styles)
     [end]
     [config] (Configuration)
     [exec] (Update Autogenerated Menu) {fluxbox-generate_menu -ds -is -t xterm -b dillo}
     #[exec] (Update XDG Menu) {mmaker -v -i -c FluxBox > ~/.fluxbox/xdg_menu}
     #[exec] (Defunct Update XDG Menu) {xdgmenumaker -f fluxbox > ~/.fluxbox/xdg_menu}
     [commanddialog] (Fluxbox Command) 
     [separator] 
     [exit] (Exit) 
   [end]
   [workspaces] (Workspaces)
[end]
